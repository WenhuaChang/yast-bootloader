([["linux", "suse", "memtest86"], ["\"New Label\"", "suse", "memtest86"]])
# Modified by YaST2. Last modification on Thu Aug 16 12:31:53 2001


boot = /dev/hda
vga = 771
read-only
menu-scheme = Wg:kw:Wg:Wg
prompt
timeout = 80
message = /boot/message

    image = /boot/vmlinuz
        label = "New Label"
        root = /dev/hda3

    image = /boot/vmlinuz.suse
        label = suse
        root = /dev/hda3
        optional

    image = /boot/memtest.bin
        label = memtest86
