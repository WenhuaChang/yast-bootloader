([["linux", "suse", "memtest86"], ["linux", "suse", "memtest86"]])
boot	= /dev/hda
vga	= 771
read-only
menu-scheme = Wg:kw:Wg:Wg
prompt
timeout	= 80
message	= /boot/message

  image  = /boot/vmlinuz
  label  = linux
  root   = /dev/hda3

  image  = /boot/vmlinuz.suse
  label  = suse
  root   = /dev/hda3
  optional

  image  = /boot/memtest.bin
  label  = memtest86

