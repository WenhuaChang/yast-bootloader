    /**
      * Reset bootloader settings
      */
    global define void Reset (boolean init) ``{
	if (Mode::autoinst ())
	    return;
	device_map = [];
	BootCommon::Reset (init);
	if (init)
	{
	    device_map = proposeDeviceMap ();
	}
	xen_removed = false;
