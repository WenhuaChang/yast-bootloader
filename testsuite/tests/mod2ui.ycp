    include "testsuite.ycp";

    include "bootloader/routines/misc.ycp";

    TEST(``(mod2ui("")), [], nil);
    TEST(``(mod2ui("simple_string")), [], nil);    
    TEST(``(mod2ui("unquoted string")), [], nil);
