{
    import "Pkg";
    include "testsuite.ycp";

    map READ_I = $[
        "target" : $[
            "size" : -1,
            "tmpdir" : "/tmp/",
	    "yast2" : nil,
        ],
        "probe" : $[
            "architecture" : "i386",
            "has_apm" : true,
            "has_pcmcia" : false,
            "has_smp" : false,
            "system" : [],
            "memory" : [],
            "cpu" : [],
            "cdrom" : $[
                "manual" : [],
            ],
            "floppy" : $[
                "manual" : [],
            ],
        ],
        "sysconfig" : $[
            "console" : $[
                "CONSOLE_FONT" : "",
                "CONSOLE_SCREENMAP" : "",
                "CONSOLE_UNICODEMAP" : "",
                "CONSOLE_MAGIC" : "",
                "CONSOLE_ENCODING" : "",
            ],
            "language" : $[
                "RC_LANG" : "",
                "DEFAULT_LANGUAGE" : "",
            ],
        ],
        "etc" : $[
            "install_inf" : $[
                "Cmdline" : "",
            ],
        ],
        "proc" : $[
            "cpuinfo" : $[
                "value" : $[
                    "0" : $[
                        "flags" : ""
                    ],
                ],
            ],
        ]
    ];

    TESTSUITE_INIT ([READ_I], 0);
    include "bootloader/routines/misc.ycp";
    TESTSUITE_CLEAN ();

    TEST(``(sformat("a\nb\nc")), [], nil);

    TEST(``(strip("")), [], nil);
    TEST(``(strip(" ")), [], nil);
    TEST(``(strip("a")), [], nil);
    TEST(``(strip("    a")), [], nil);
    TEST(``(strip("a    ")), [], nil);
    TEST(``(strip("a   a")), [], nil);
    TEST(``(strip("   a ")), [], nil);
    TEST(``(sformat("%1", [strip(" \t  a ")])), [], nil);
    TEST(``(sformat("%1", [strip(" \n   \n\t a\n  a\n ")])), [], nil);
    TEST(``(sformat("%1", [strip("\t\t\t    s \t a\n ")])), [], nil);
}
